@model Benefit.Web.Models.Admin.SellerViewModel

<div class="form-group">
    <label class="control-label no-padding-right">Title</label>
    <div>
        @Html.TextBoxFor(m => m.Seller.Title, new { @class = "width-50" })
    </div>
</div>
<div class="form-group">
    <label class="control-label no-padding-right">Google site verification token</label>
    <div>
        @Html.TextBoxFor(m => m.Seller.GoogleSiteVerificationToken, new { @class = "width-50" })
    </div>
</div>
<div class="form-group">
    <label class="control-label no-padding-right">Альтернативний текст зображень</label>
    <div>
        @Html.TextBoxFor(entry => entry.Seller.AltText, new { @class = "width-50" })
    </div>
</div>
<div class="form-group">
    <label class="control-label no-padding-right">Короткий опис для SEO (meta description)</label>
    <div>
        @Html.TextAreaFor(entry => entry.Seller.ShortDescription, new { cols = 70, rows = 5, @class = "width-50" })
    </div>
</div>
<div class="form-group">
    <label class="control-label no-padding-right">Seo суфікс</label>
    <div>
        @Html.TextAreaFor(entry => entry.Seller.SeoSuffix, new { cols = 70, rows = 5, @class = "width-50" })
    </div>
</div>
