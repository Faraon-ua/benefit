@using Benefit.DataTransfer.ViewModels
@using Benefit.Domain.Models

@{
    ViewBag.Title = "Про нас";
    Layout = "_SellerLayout.cshtml";
    var seller = ViewBag.Seller as Seller;
}

<div class="col-md-8 col-xl-9 content">

    @Html.Partial("~/views/sellerarea/Megashop/_BreadcrumbsPartial.cshtml", new BreadCrumbsViewModel() { Page = new InfoPage() { Name = "Про нас" } })

    <div class="fn_ajax_content margin-top-10">
        <div class="block">
            <h1 class="h1">
                <span data-page="3">Про нас</span>
            </h1>
            <div class="padding">
                @Html.Raw(seller.Description)
            </div>
        </div>
    </div>
</div>
